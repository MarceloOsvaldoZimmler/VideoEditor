<?xml version="1.0" encoding="utf-8"?>
<moviemasher config='media/xml/control_nav_module.xml'>
	
	<!-- MEDIA TYPE FILTER CONTROLS -->
	<control style='text,tab' preselected="1" pattern='Video' select='browser.source=video_source' bind="browser.source" value="video_source" />
	<control style='text,tab' pattern='Audio' select='browser.source=audio_source' bind='browser.source' value='audio_source' />
	<control style='text,tab' pattern='Images' select='browser.source=image_source' bind='browser.source' value='image_source' />
	
</moviemasher>