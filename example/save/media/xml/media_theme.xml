	<!-- TITLE -->
	<media id='titletheme' group='theme' type='theme' label='Title' 
		editable='font,forecolor,backcolor,longtext,textsize' 
		forecolor='CCCCCC' backcolor='333333'
		longtext='Editable Text' 
		textsize='14' font='default'
		symbol='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@Title' 
		icon='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@com.moviemasher.module.Title' 
	/>

	<!-- CAPTION -->
	<media group='theme' type='effect' id='captioneffect' label='Caption'
		symbol='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@Caption' 
		icon='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@com.moviemasher.module.Caption' 
		padding='3'
		longtext='Caption'
		forecolor='000000' backalpha='50' backcolor='FFFFFF' textsize='14' font='default'
		editable='padding,font,textsize,longtext,forecolor,backcolor,backalpha'
	/>
	
	<!-- TEXTBOX -->
	<media group='theme' type='effect' id='textboxeffect' label='Textbox'
		symbol='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@Textbox' 
		icon='{mm_path}moviemasher/com/moviemasher/module/All/stable.swf@com.moviemasher.module.Textbox' 
		longtext='Textbox'
		fade='0,0,10,100,90,100,100,0' 
		forecolor='000000' backalpha='50' backcolor='FFFFFF' textsize='14' font='default'
		scale='50,50' position='50,50'
		editable='fade,font,scale,position,textsize,longtext,forecolor,backalpha,backcolor'
	/>
	
